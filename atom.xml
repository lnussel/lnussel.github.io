---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>lnussel -- random thoughts</title>
	<link href="http://lnussel.github.io/atom.xml" rel="self"/>
	<link href="http://lnussel.github.io/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://lnussel.github.io</id>
	<author>
		<name>Ludwig Nussel</name>
		<email>lnussel@suse.de</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="http://lnussel.github.io{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://lnussel.github.io{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}
</feed>
